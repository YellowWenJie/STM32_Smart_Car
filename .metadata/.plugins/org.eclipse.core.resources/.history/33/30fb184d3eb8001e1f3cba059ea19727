#include "main.h"

typedef enum
{
  Front_Left,
  Front_Right,
  Back_Left,
  Back_Right
} Motor_Single;

int myabs(uint32_t num);
void Motor(Motor_Single motor_single, int num);
void Set_Pwm(Motor_Single motor_single, int pwm);

